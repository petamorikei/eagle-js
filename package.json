{
  "name": "@petamorikei/eagle-js",
  "version": "0.1.4",
  "description": "Wrapper for Eagle API",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node src/main.ts",
    "dev:watch": "ts-node-dev --respawn src/main.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node .",
    "check": "biome check --write .",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [],
  "author": "Petamori Kei",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/node": "^22.7.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  }
}
