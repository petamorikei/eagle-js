{
  "name": "@petamorikei/eagle-js",
  "version": "0.2.0",
  "description": "Wrapper for Eagle API",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/main.ts",
    "dev:watch": "tsx watch src/main.ts",
